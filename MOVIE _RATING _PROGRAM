package loops;
import java.util.Scanner;
public class ForLoops {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
System.out.println("Enter The Rating You Wish To Give");
		int rating = sc.nextInt();
		switch (rating) {
		case 1:
		case 2:
			System.out.println("Bad Review/Bekaar Hai");
			break;
		case 3 :
			System.out.println("Average Review/One TIme Watch!");
			break;
		case 4:
		case 5:
			System.out.println("Best Review/badhiya Picture");
		}
	}

}
